{
  "name": "kironuniversity/main-website",
  "description": "Kiron Main Website",
  "homepage": "https://kiron.ngo",
  "keywords": ["october", "cms", "octobercms", "laravel", "kiron", "education", "refugees"],
  "require": {
    "php": ">=5.5.9",
    "october/rain": "~1.0",
    "october/system": "~1.0",
    "october/backend": "~1.0",
    "october/cms": "~1.0",
    "laravel/framework": "5.1.*",
    "wikimedia/composer-merge-plugin": "dev-master",
    "illuminate/html": "5.0.*",
    "cpliakas/git-wrapper": "^1.7"
  },
  "require-dev": {
    "fzaninotto/faker": "~1.4",
    "phpunit/phpunit": "~4.0",
    "phpunit/phpunit-selenium": "~1.2"
  },
  "autoload-dev": {
    "classmap": [
      "tests/TestCase.php",
      "tests/UiTestCase.php",
      "tests/PluginTestCase.php"
    ]
  },
  "scripts": {
    "post-create-project-cmd": [
      "php artisan key:generate"
    ]
  },
  "config": {
    "preferred-install": "dist"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "extra": {
    "merge-plugin": {
      "include": [
        "plugins/*/*/composer.json"
      ],
      "recurse": true,
      "replace": false,
      "merge-dev": false
    }
  }
}
